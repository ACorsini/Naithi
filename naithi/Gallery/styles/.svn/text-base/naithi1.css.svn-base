@import url("layout.css");
body {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 0.8em;
	background-color: #6F7C71;
	background-attachment: scroll;
}
#container {
	width: 750px;
	margin: 10px auto;
	background-color: #6F7C71;
}
#masthead {
	text-align: left;
	width: 740px;
	background-color: #98BA9E;
	height: 220px;
	padding-left: 5px;
	padding-top: 5px;
	margin-top: 40px;
	background-attachment: scroll;
}
#navigation {
	position: relative;
	float: left;
	width: 140px;
	margin-top: 10px;
	margin-bottom: 10px;
	background-color: #98BA9E;
}
#content {
	float: left;
	width: 568px;
	margin-top: 10px;
	margin-bottom: 10px;
	margin-left: 10px;
	padding: 10px;
	background-color: #fff;
}
#footer {
	text-align: center;
	clear: both;
	width: 750px;
	padding: 0px;
	background-color: #98BA9E;
}
/* Masthead Styles */
.h3masthead {
	font-size: 1.5em;
	font-weight: bold;
	vertical-align: bottom;
	text-align: left;
	padding-top: 120px;
}
.masthead img {
	float: right;
	padding-top: 10px;
}
.youth {
	float: right;
	padding-top: 15px;
}
.nlogo {
	position: relative;
	float: left;
	top: 5px;
	left: 10px;
	width: 363px;
}
/* Navigation Styles */
#navigation ul {
	list-style-type: none;
	width: 100%;
	display: block;
	margin: 0;
	padding: 0;
}
#navigation li {
	display: block;
	margin: 0;
	padding: 0;
	border: 1px solid #98BA9E;
	background-color: #98BA9E;
}
#navigation a {
	color: black;
	text-decoration: none;
	display: block;
	padding: 5px;
	border: 1px solid #98BA9E;
}
#navigation a:hover {
	color: #fff;
	text-decoration: none;
	border: 1px solid #964;
	background-color: #964;
}
/* Content Styles */
#content h1,h2,h3,h4,h5,h6 {
	color: #340E70;
}
#content img {
	padding: 5px;
	border: 2px solid #964;
	margin-bottom: 10px;
	margin-right: 10px;
	margin-top: 10px;
}
/* Footer Styles */
#footer p {
	color: #340E70;
	margin-top: 5px;
	margin-bottom: 5px;
	padding-top: 15px;
	padding-right:15px;
	padding-bottom: 20px;
	text-align: right;
	background-color: #98BA9E;
	clear: both;
}
.clogo {
	float: left;
	margin-left:15px;
}
#footer a {
	color: #230D9F;
	text-decoration: underline;
}
#footer a:hover {
	color: black;
	text-decoration: none;
}
a {
	color: #a60;
	}
a:hover {
	color: #742;
	text-decoration: underline;
	background-color:orange;
}
.style_bold {
	font-weight: bold;
}
.style_italic {
	font-style: italic;
}
.floatR {
	float: right;
	margin: 10px;
}
.floatL {
	position: relative;
	margin-right: 20px;
	margin-top: 10px;
	float: left;
}
.notes {
    font-size: 0.8em;
    text-align: center;
    color: White;
}   
    
/* Books Styles */

#books img: {
	float: right;
	margin: 10px;
}
a {
	color: #000000;
}
.a:hover {
	color: #F8F5C7;
}
#cds img {
	margin-right: 20px;
	margin-top: 10px;
	float: left;
}
.highlite {
	padding: 10px;
	margin-top: 10px;
	border-style: none;
	text-align: center;
	white-space: normal;
	font-weight: bold;
	color: red;
	background-color: #FFFF00;
}
.class {
	margin-top: 10px;
}
#full {
	font-weight: bold; 
	line-height: 1.2em;
	text-align: center;
	padding: 3px 3px 5px 3px;
	margin-top: 10px;
	margin-bottom: 10px;
	color: red;
	background-color: #FFFF00;
}
/* Table styles */
.office {
	font-weight: bold;
	text-align: right;
}
.committee {
	padding-left: 10px;
}
.style1 {
	border-style: solid;
	border-width: 1px;
	background-color: #98BA9E;
}
#table1 {
	border: 3px solid #6F7C71;
	background-color: #98BA9E;
	margin-left: 60px;
}
#table2 {
	margin-left: 0px;
}
